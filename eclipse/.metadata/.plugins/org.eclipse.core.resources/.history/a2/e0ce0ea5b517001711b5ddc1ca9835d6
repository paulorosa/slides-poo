package Atividade1;

/**
 *
 * @author Anderson James
 */
public class Fibonnacci {
  public  int aux;
   public int a ;
   public int b ;

    public Fibonnacci(int a , int b ,int aux) {
       this.a = a;
   
       this.aux = aux;

      this.b = b;  
    }

    public void proximoValor() {
        
aux = a;
a = a+ b; 
b=aux ; 
  
       
        
    }

    public int valorAtual() {
      return a;
    }
}
