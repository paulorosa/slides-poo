
public class PolinomioGeral {
	private int valorAtual;
	private int posicao;

	private int[] coeficientes;

	public PolinomioGeral(int[] coeficientes) {
		this.coeficientes = coeficientes;
		this.posicao = 0;

		this.valorAtual = calcula();
	}

	public void proximoValor() {
		this.posicao++;
		this.valorAtual = calcula();
	}

	public int valorAtual() {
		return this.valorAtual;
	}

	private int calcula() {
		int valorParcial = 0;
		for (int i=0; i<coeficientes.length; i++) {
			valorParcial += Math.pow(posicao, i);
		}
		return valorParcial;
	}
	
	 public static void main (String[] args) {
	        int quantidade = Integer.parseInt(args[0]);

	        
			int coeficienteGrauDois = Integer.parseInt(args[1]);
			int coeficienteGrauUm = Integer.parseInt(args[2]);
			int coeficienteGrauZero = Integer.parseInt(args[3]);

			PolinomioGeral polinomio =  new PolinomioGeral(coeficienteGrauDois,coeficienteGrauUm,coeficienteGrauZero);

	        for (int i=0; i<quantidade; i++) {
	            System.out.print (polinomio.valorAtual() + " ");
	            polinomio.proximoValor();
	        }
	    }    
}